# CHATAO.cz - ENV šablona
# Zkopírujte tento soubor jako .env.local a vyplňte své hodnoty

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here-keep-secret

# Google Analytics
GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Instagram API (pro budoucí integraci)
INSTAGRAM_APP_ID=
INSTAGRAM_APP_SECRET=
INSTAGRAM_REDIRECT_URI=https://chatao.cz/auth/instagram/callback

# Volitelné - Email pro notifikace
NOTIFICATION_EMAIL=info@chatao.cz

# Volitelné - Sentry (error tracking)
SENTRY_DSN=

# Environment
NODE_ENV=production
